set drvpath=%loadroot%%%loadfile%
::参数不为iso则特殊处理
if /i not "%~2"=="iso" && set noisopar=1
::处理SRS 位于fd0
call Fn.15 "%srspath%" "" || goto :srs_end
call :find %srspath% || goto :ferr
